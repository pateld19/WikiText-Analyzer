@DECL

IsPOS(L("word")) {
	L("word") = strtolower(L("word"));

	if (L("word") == "noun")
		return 1;
	
	if (L("word") == "verb")
		return 1;

	if (L("word") == "interjection")
		return 1;

	return 0;
}

@@DECL
